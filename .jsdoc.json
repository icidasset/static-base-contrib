{
    "source": {
        "include": "src/",
        "includePattern": ".js$"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash/",
        "encoding": "utf8",
        "destination": "./",
        "recurse": true,
        "verbose": true
    }
}
